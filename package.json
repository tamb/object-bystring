{
  "name": "object-bystring",
  "version": "7.0.3",
  "description": "Access and alter objects using string literals",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tamb/object-bystring.git"
  },
  "keywords": [
    "object",
    "bystring",
    "string",
    "utility",
    "mutable"
  ],
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "files": [
    "LICENSE",
    "README.md",
    "dist/index.js",
    "dist/index.esm.js",
    "dist/index.esm.min.js",
    "dist/index.d.ts",
    "dist/index.d.ts.map"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test.performance": "node performance-test.mjs",
    "build": "node clean.mjs && npx rollup -c rollup.config.mjs",
    "safe-publish": "npm run test && npm run build && npm publish"
  },
  "author": "tamb <tamb@protonmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tamb/object-bystring/issues"
  },
  "homepage": "https://github.com/tamb/object-bystring#readme",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.47.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
