{
  "name": "object-bystring",
  "version": "6.1.0",
  "description": "Access and alter objects using string literals",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tamb/object-bystring.git"
  },
  "keywords": [
    "object",
    "bystring",
    "string",
    "utility",
    "mutable"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "dist/index.d.ts",
    "dist/index.d.ts.map",
    "dist/index.js"
  ],
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "test": "jest src/",
    "test.watch": "jest --watch src/",
    "test.performance": "node performance-test.js",
    "build": "node clean && npx rollup -c rollup.config.js",
    "safe-publish": "npm run test && npm run build && npm publish"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "author": "tamb <tamb@protonmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tamb/object-bystring/issues"
  },
  "homepage": "https://github.com/tamb/object-bystring#readme",
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@types/jest": "^29.5.12",
    "husky": "^4.2.5",
    "jest": "^29.7.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.11.2",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
